{
  "name": "how-npm-works-docs",
  "version": "1.0.0",
  "description": "gitbook for how npm works docs",
  "main": "index.js",
  "scripts": {
    "install": "gitbook install",
    "start": "gitbook serve",
    "build": "npm run build:styles && npm run concat:styles && gitbook build",
    "build:styles": "dr-frankenstyle ./styles",
    "concat:styles": "cat ./styles/components.css ./styles/custom.css > ./styles/website.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/how-npm-works-docs.git"
  },
  "keywords": [],
  "author": "",
